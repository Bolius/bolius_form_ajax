{
    "name": "bolius/bolius-form-ajax",
    "description": "Ajax form extension.",
    "type": "typo3-cms-extension",
    "license": [
        "GPL-2.0+"
    ],
    "authors": [
        {
            "name": "Bolius Digital",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "config": {
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "composer/installers": true,
            "typo3/cms-composer-installers": true
        }
    },
    "replace": {
        "typo3-ter/bolius-form-ajax": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "bolius_form_ajax"
        }
    }
}